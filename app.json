{
  "expo": {
    "name": "B10 News",
    "slug": "b10-news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "scheme": "b10news",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.b10news.app"
    },
    "android": {
      "package": "com.b10news.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "targetSdkVersion": 35,
      "minSdkVersion": 21
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/app-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "image": "./assets/app-icon.png",
            "backgroundColor": "#FFFFFF"
          }
        }
      ],
      "expo-web-browser",
      "expo-video",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Mandali-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "da9193a0-9f66-4f74-a545-bf4649c23bca"
      },
      "firebaseApiKey": "$EXPO_PUBLIC_FIREBASE_API_KEY",
      "firebaseAuthDomain": "$EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN",
      "firebaseProjectId": "$EXPO_PUBLIC_FIREBASE_PROJECT_ID",
      "firebaseStorageBucket": "$EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET",
      "firebaseMessagingSenderId": "$EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID",
      "firebaseDatabaseURL": "https://$EXPO_PUBLIC_FIREBASE_PROJECT_ID.firebaseio.com",
      "projectId": "da9193a0-9f66-4f74-a545-bf4649c23bca"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/app-icon.png",
          "color": "#FFFFFF"
        }
      ]
    ]
  }
}
